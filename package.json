{
  "name": "@built-fast/vector-pro-sdk",
  "version": "0.1.0",
  "description": "Node.js SDK for the Vector Pro API by BuiltFast",
  "homepage": "https://builtfast.dev/oss/vector-pro-sdk-node/",
  "repository": {
    "type": "git",
    "url": "https://github.com/built-fast/vector-pro-sdk-node"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vector",
    "vector-pro",
    "builtfast",
    "wordpress",
    "hosting",
    "api",
    "sdk"
  ],
  "author": "BuiltFast (https://builtfast.dev)",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
